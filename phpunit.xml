<phpunit>
  <testsuites>
    <testsuite name="UserSystem">
      <directory>Tests</directory>
    </testsuite>
  </testsuites>
  <logging>
    <log type="coverage-clover" target="build/logs/clover.xml"/>
  </logging>
  <whitelist processUncoveredFilesFromWhitelist="true">
    <directory suffix=".php">/UserSystem</directory>
  </whitelist>
</phpunit>
